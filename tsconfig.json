{
	"compilerOptions": {
		"target": "es5",
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": true,
		"downlevelIteration": true,
		"skipLibCheck": true,
		"strict": true,
		"noEmit": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "bundler",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"incremental": true,
		"baseUrl": "./",
		"paths": {
			"@/*": ["src/*"],
			"@/public/*": ["public/*"]
		}
	},
	"include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/components/contextTable", "src/**/*.tsx"],
	"exclude": ["node_modules"]
}
